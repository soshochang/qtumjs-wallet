{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "no-var-requires": false,
    "semicolon": [
      true,
      "never"
    ],
    "max-classes-per-file": false,
    "object-literal-sort-keys": false,
    "variable-name": [
      true,
      "allow-leading-underscore"
    ],
    "ordered-imports": false,
    "no-console": false,
    "no-namespace": false,
    "no-module": false,
    "space-before-function-paren": false
  },
  "rulesDirectory": []
}
